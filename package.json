    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/mime": "^4.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.13.5",
    "@types/qs": "^6.9.18",
    "@types/serve-static": "^1.15.7",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "globals": "^15.15.0",
